.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.textStyle {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.textContainer {
    margin-top: 200px;
}

.api-item {
    padding: 20rpx;
    background-color:#ffffff;
    border-bottom: 2rpx solid #cccccc; 
}

.api-title {
    background-color: #ffffff;
    text-align: left;
    padding: 20rpx;
    font-size: 48rpx;
}